<div class="container">
<table class="table table-bordered">
	<thead>
		<tr>
			<th>Id</th>
			<th>Email</th>
			<th>Date Time</th>
	</thead>
	<tbody>
		<% @users.each do |user| %>
		<tr>
			<td><%= user.id %></td>
			<td><%= user.email %></td>
			<td><%= user.created_at %></td>
		</tr>
		<% end %>
	</tbody>
</table>
</div>